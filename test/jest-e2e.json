{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "collectCoverageFrom": [
    "src/modules/**/*.ts"
  ],
  "coveragePathIgnorePatterns": [
    ".entity.ts",
    ".module.ts",
    ".repository.ts"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
